{
  "name": "jspteroapi",
  "version": "1.10.0",
  "description": "A pterodactyl v1 api using undici",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc-watch --onCompilationComplete \"node --use-openssl-ca dist/test.js\"",
    "build": "rimraf dist/* && tsc",
    "lint": "eslint \"src/**\"",
    "docs:build": "typedoc --categorizeByGroup --defaultCategory Types --excludeInternal --name JSPteroAPI",
    "release": "yarn build && np"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Linux123123/JSPteroAPI.git"
  },
  "keywords": [
    "Pterodactyl",
    "API",
    "undici",
    "node-js",
    "js",
    "ptero-api",
    "ptero",
    "pterodactyl-api",
    "API client"
  ],
  "author": "Linux123123",
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "bugs": {
    "url": "https://github.com/Linux123123/JSPteroAPI/issues"
  },
  "homepage": "https://jspteroapi.linux123123.com",
  "dependencies": {
    "undici": "^5.9.1",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@microsoft/tsdoc-config": "^0.16.1",
    "@types/node": "^18.7.11",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.16",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tsc-watch": "^5.0.3",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4"
  }
}
